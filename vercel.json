{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        },
        {
            "src": "/(.*)",
            "methods":[
                "POST",
                "GET",
                "PUT",
                "DELTE",
                "PATCH"
            ],
            "dest":"/src/index.js"
        }
    ]
}