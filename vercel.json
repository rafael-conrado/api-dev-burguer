{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@now/node"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/src" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
  }


